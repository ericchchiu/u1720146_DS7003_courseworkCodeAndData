dfEach4RWdNoOfOccu = function (x, y, z) {
for (i in 5: nrow(y)) {
if (i%%4 == 1) {
df <- y[i,]
}
else if (i%%4 != 0) {
df <- rbind(df, y[i,])
}
else {
df <- rbind(df, y[i,])
z = rbind(z, A(x, df))
}}
return(z)
}#ok
